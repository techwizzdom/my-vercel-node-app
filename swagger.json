{
  "openapi": "3.0.0",
  "info": {
    "title": "My Vercel Node App API",
    "version": "1.0.0",
    "description": "A simple API to get users"
  },
  "servers": [
    {
      "url": "https://my-vercel-node-app.vercel.app"
    }
  ],
  "paths": {
    "/api/get-users": {
      "get": {
        "summary": "Get users",
        "responses": {
          "200": {
            "description": "A list of users",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "integer",
                        "example": 1
                      },
                      "name": {
                        "type": "string",
                        "example": "John Doe"
                      },
                      "email": {
                        "type": "string",
                        "example": "john.doe@example.com"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
